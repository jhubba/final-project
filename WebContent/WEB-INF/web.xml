<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
	id="WebApp_ID" version="3.1"
>
    
<display-name>Bull-Notes</display-name>
  
<filter>
	<filter-name>struts2</filter-name>
	<filter-class>org.apache.struts2.dispatcher.filter.StrutsPrepareAndExecuteFilter</filter-class>	
</filter>

<filter-mapping>
	<filter-name>struts2</filter-name>
	<url-pattern>*.action</url-pattern>
	<url-pattern>/login.jsp</url-pattern>
	<url-pattern>/login</url-pattern>
	<url-pattern>/loadWatchList</url-pattern>
	<url-pattern>/userprofile.jsp</url-pattern>	
	<url-pattern>/searchSymbol</url-pattern>
	<url-pattern>/createaccount.jsp</url-pattern>	
	<url-pattern>/createAccount</url-pattern>	
	<url-pattern>/searchSymbolUP</url-pattern>
	<url-pattern>/addSymbolToWatchList</url-pattern>
</filter-mapping>
  
 <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
</welcome-file-list>  

</web-app>